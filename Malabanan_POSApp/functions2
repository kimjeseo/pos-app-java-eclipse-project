    final JButton TOTALZ = new JButton("TOTAL");
    TOTALZ.addActionListener(new ActionListener() {
          public void actionPerformed(ActionEvent e) {
            sample_POS.this.itemcost[0] = Double.parseDouble(sample_POS.this.HOWMANYRH.getText()) * 45.0D;
            sample_POS.this.itemcost[1] = Double.parseDouble(sample_POS.this.HOWMANYGIN.getText()) * 40.0D;
            sample_POS.this.itemcost[2] = Double.parseDouble(sample_POS.this.HOWMANYCOKE.getText()) * 20.0D;
            sample_POS.this.itemcost[3] = Double.parseDouble(sample_POS.this.HOWMANYROYAL.getText()) * 20.0D;
            sample_POS.this.itemcost[4] = Double.parseDouble(sample_POS.this.HOWMANYYOSIWHITE.getText()) * 8.0D;
            sample_POS.this.itemcost[5] = Double.parseDouble(sample_POS.this.HOWMANYSISIG.getText()) * 105.0D;
            sample_POS.this.itemcost[6] = Double.parseDouble(sample_POS.this.HOWMANYADOBO.getText()) * 75.0D;
            sample_POS.this.itemcost[7] = Double.parseDouble(sample_POS.this.HOWMANYBURGER.getText()) * 55.0D;
            sample_POS.this.itemcost[8] = Double.parseDouble(sample_POS.this.HOWMANYEXEGG.getText()) * 10.0D;
            sample_POS.this.itemcost[9] = Double.parseDouble(sample_POS.this.HOWMANYEXSOUP.getText()) * 10.0D;
            sample_POS.this.itemcost[10] = Double.parseDouble(sample_POS.this.HOWMANYSML.getText()) * 40.0D;
            sample_POS.this.itemcost[11] = Double.parseDouble(sample_POS.this.HOWMANYBLACKLABEL.getText()) * 1005.0D;
            sample_POS.this.itemcost[12] = Double.parseDouble(sample_POS.this.HOWMANYSPRITE.getText()) * 20.0D;
            sample_POS.this.itemcost[13] = Double.parseDouble(sample_POS.this.HOWMANYMARLRED.getText()) * 8.0D;
            sample_POS.this.itemcost[14] = Double.parseDouble(sample_POS.this.HOWMANYMARLBLUE.getText()) * 8.0D;
            sample_POS.this.itemcost[15] = Double.parseDouble(sample_POS.this.HOWMANYSINIGANG.getText()) * 105.0D;
            sample_POS.this.itemcost[16] = Double.parseDouble(sample_POS.this.HOWMANYPORKCHOP.getText()) * 95.0D;
            sample_POS.this.itemcost[17] = sample_POS.this.itemcost[0] + sample_POS.this.itemcost[1] + sample_POS.this.itemcost[2] + sample_POS.this.itemcost[3] + sample_POS.this.itemcost[4] + sample_POS.this.itemcost[5] + sample_POS.this.itemcost[6];
            sample_POS.this.itemcost[18] = sample_POS.this.itemcost[7] + sample_POS.this.itemcost[8] + sample_POS.this.itemcost[9] + sample_POS.this.itemcost[10] + sample_POS.this.itemcost[11] + sample_POS.this.itemcost[12] + sample_POS.this.itemcost[13];
            sample_POS.this.itemcost[19] = sample_POS.this.itemcost[14] + sample_POS.this.itemcost[15] + sample_POS.this.itemcost[16];
            sample_POS.this.itemcost[20] = sample_POS.this.itemcost[17] + sample_POS.this.itemcost[18] + sample_POS.this.itemcost[19];
            sample_POS.this.ETAX = String.format("P %.2f", new Object[] { Double.valueOf(this.this$0.itemcost[20] / 100.0D) });
            sample_POS.this.ESUBTOTAL = String.format("P %.2f", new Object[] { Double.valueOf(this.this$0.itemcost[20]) });
            sample_POS.this.ETOTAL = String.format("P %.2f", new Object[] { Double.valueOf(this.this$0.itemcost[20] + this.this$0.itemcost[20] / 100.0D) });
            sample_POS.this.TAX.setText(sample_POS.this.ETAX);
            sample_POS.this.SUBTOTAL.setText(sample_POS.this.ESUBTOTAL);
            sample_POS.this.TT.setText(sample_POS.this.ETOTAL);
            textArea.setEnabled(true);
            int refs = 1000 + (int)(Math.random() * 5123.0D);
            Calendar timer = Calendar.getInstance();
            timer.getTime();
            SimpleDateFormat tTime = new SimpleDateFormat("hh : mm : ss");
            tTime.format(timer.getTime());
            SimpleDateFormat tDate = new SimpleDateFormat("MMM : dd : YYYY");
            tDate.format(timer.getTime());
            textArea.append("\t Welcome to TAMBAYKAINAN\nReference: \t\t" + refs + 
                " \n=============================================\t " + 
                
                "\n=============================================\t" + 
                "\nTax: \t\t\t" + sample_POS.this.ETAX + 
                "\nSub: \t\t\t" + sample_POS.this.ESUBTOTAL + 
                "\nTotal: \t\t\t" + sample_POS.this.ETOTAL + 
                "\n=============================================\t" + 
                "\nDate: " + tDate.format(timer.getTime()) + 
                "\tTime: " + tTime.format(timer.getTime()) + 
                "\n\nThank You For Coming at TAMBAYKAINAN\n" + 
                "\n NAME:  " + sample_POS.this.textNAME.getText());
            if (e.getSource() == TOTALZ) {
              sample_POS.this.NAME = "";
              sample_POS.this.ORDERS = "";
              sample_POS.this.NAME = sample_POS.this.textNAME.getText().trim();
              sample_POS.this.ORDERS = sample_POS.this.ETOTAL.trim();
              sample_POS.summery = "COST:  " + sample_POS.this.ORDERS + 
                sample_POS.this.itemcost[20];
              String Data = sample_POS.summery;
              try {
                BufferedWriter reader = new BufferedWriter(new FileWriter(new File("C:\\Users\\pc amsel\\Desktop\\ORDERS.txt"), true));
                reader.write(Data);
                reader.newLine();
                reader.close();
                JOptionPane.showMessageDialog(sample_POS.this.frmTambaykainan, "ORDER COMPLETE ");
              } catch (IOException E) {
                System.out.println("ERROE" + E);
              } 
            } 
          }
        });
    TOTALZ.setBounds(44, 580, 89, 43);
    panel_2.add(TOTALZ);
